<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.artop.aal.examples.common.ui.newWizards.category.artopExampleWizards"
            class="org.eclipse.sphinx.graphiti.workspace.ui.wizards.CreateDiagramWizard"
            icon="icons/wizban/pid16.gif"
            id="org.eclipse.sphinx.graphiti.workspace.ui.newWizards.graphitiDiagram"
            name="%sphinx.graphiti.workspace.ui.newWizards.graphitiDiagram.name">
         <description>
            %sphinx.graphiti.workspace.ui.newWizards.graphitiDiagram.description
         </description>
         <selection
               class="org.eclipse.core.resources.IContainer">
         </selection>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.sphinx.graphiti.workspace.ui.editors.BasicGraphitiDiagramEditor"
            contributorClass="org.eclipse.graphiti.ui.editor.DiagramEditorActionBarContributor"
            default="true"
            extensions="diag"
            icon="icons/diagram.gif"
            id="org.eclipse.sphinx.graphiti.workspace.ui.editors.graphitiDiagram"
            matchingStrategy="org.eclipse.graphiti.ui.editor.DiagramEditorFactory$DiagramEditorMatchingStrategy"
            name="%sphinx.graphiti.workspace.ui.editors.graphitiDiagram.name">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.elementFactories">
      <factory
            class="org.eclipse.sphinx.graphiti.workspace.ui.BasicDiagramEditorFactory"
            id="org.eclipse.sphinx.graphiti.workspace.ui.BasicDiagramEditorFactory">
      </factory>
   </extension>

</plugin>
