<plugin>
   <extension
         point="org.eclipse.sphinx.emf.metaModelDescriptors">
      <descriptor
            class="org.eclipse.sphinx.examples.uml2.ide.metamodel.UML2MMDescriptor"
            id="org.eclipse.sphinx.examples.uml2">
      </descriptor>
   </extension>
      
   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser 
	         type="uml"
	         class="org.eclipse.sphinx.examples.uml2.ide.internal.ExtendedUMLResourceFactoryImpl" >
      </parser>
   </extension>
      
   <extension point="org.eclipse.emf.ecore.content_parser">
      <parser 
      	     contentTypeIdentifier="org.eclipse.uml2.uml_4_0_0"
             class="org.eclipse.sphinx.examples.uml2.ide.internal.ExtendedUMLResourceFactoryImpl" >
       </parser>
  </extension>

</plugin>
